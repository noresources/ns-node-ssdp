{
	"env": {
		"es6": true, 
		"es2021": true, 
		"commonjs": true, 
		"node": true, 
		"browser": true
	}, 
	"parserOptions": {
		"sourceType": "module", 
		"ecmaVersion": 2020
	}, 
	"rules": {
		"no-console": "off", 
		"keyword-spacing": [
			"error", 
			{
				"after": true
			}
		], 
		"indent": [
			"error", 
			"tab"
		], 
		"brace-style": [
			"error", 
			"1tbs"
		], 
		"quotes": [
			"error", 
			"single"
		], 
		"key-spacing": [
			"error", 
			{
				"beforeColon": false, 
				"afterColon": true, 
				"mode": "strict"
			}
		], 
		"curly": [
			"error", 
			"all"
		], 
		"linebreak-style": [
			"error", 
			"windows"
		], 
		"semi": [
			"error", 
			"always"
		], 
		"object-property-newline": [
			"error"
		], 
		"no-mixed-spaces-and-tabs": "off", 
		"no-trailing-spaces": [
			"error", 
			{
				"skipBlankLines": true
			}
		], 
		"no-var": [
			"error"
		], 
		"quote-props": [
			"error", 
			"always"
		], 
		"no-multiple-empty-lines": 1
	}, 
	"extends": "eslint:recommended", 
	"globals": {
	}
}