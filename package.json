{
	"name": "@noresources/ssdp",
	"version": "1.0.0",
	"description": "SSDP protocol implementation",
	"main": "./index.cjs",
	"scripts": {
		"test": "tape tests/tape/*.mjs",
		"doc": "jsdoc --recurse -c resources/jsdoc/configuration.json --readme README.md --tutorials resources/jsdoc/tutorials --destination doc src/",
		"lint": "eslint -c resources/eslint/configuration.mjs --fix src/*.mjs",
		"update-package-info": "node tools/update-package-info.mjs "
	},
	"keywords": [
		"SSDP"
	],
	"author": "Renaud Guillard",
	"license": "MIT",
	"type": "module",
	"directories": {
		"src": "./src"
	},
	"devDependencies": {
		"tape": "^5.6.0"
	},
	"dependencies": {
		"ip": "^2.0.1"
	},
	"files": [
		"package.mjs",
		"index.mjs",
		"index.cjs"
	],
	"exports": {
		".": [
			{
				"import": "./index.mjs",
				"require": "./index.cjs"
			},
			"./index.cjs"
		],
		"./package.json": "./package.json",
		"./platform/node": [
			{
				"import": "./src/platform/node/index.mjs",
				"require": "./src/platform/node/index.cjs"
			},
			"./src/platform/node/index.cjs"
		],
		"./platform": [
			{
				"import": "./src/platform/index.mjs",
				"require": "./src/platform/index.cjs"
			},
			"./src/platform/index.cjs"
		]
	},
	"module": "./index.mjs"
}